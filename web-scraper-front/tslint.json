{
  "defaultSeverity": "warning",
  "extends": [
    "tslint:recommended"
  ],
  "linterOptions": {
    "exclude": [
      "node_modules/**",
      "translations.json"
    ]
  },
  "rules": {
    "quotemark": [
      false,
      "single"
    ],
    "indent": [
      false,
      "spaces",
      2
    ],
    "no-console": false,
    "interface-name": false,
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-consecutive-blank-lines": false
  }
}
